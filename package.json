{
  "name": "go-test",
  "version": "1.0.0",
  "description": "Example using react and gin",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "client-build": "webpack --mode production",
    "client-bundle": "webpack --mode development --config ./webpack.config.js",
    "client-watch": "webpack --watch --progress --config ./webpack.config.js",
    "client-test": "mocha --reporter spec \"client/**/*.test.js\" --require @babel/register --require ./client/testSetup.js",
    "server-install": "cd server && dep ensure && cd ..",
    "server-run": "go run server/main.go",
    "install-all": "yarn run server-install && yarn install",
    "dev": "concurrently -k -p '{time} {name}:' -t 'HH:mm' -n 'Client,Server' -c 'yellow,magenta' 'yarn client-watch' 'yarn server-run'"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-react": "^7.11.1",
    "expect": "^23.6.0",
    "jsdom": "^13.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "mocha": "^5.2.0",
    "nock": "^10.0.2",
    "react-router-dom": "^4.3.1",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "connected-react-router": "^4.5.0",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "toastr": "^2.1.4"
  }
}
